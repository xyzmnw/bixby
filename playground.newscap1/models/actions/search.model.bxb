action (search) {
  type(Search)
  description (search the news for the query)
  collect {
    
    input (language) {
      type (Language)
      min (Optional) max (One)
    }
    input(q)
    {
      type (Q)
      min (Required)max (Many)
    }
    input (domains)
    {
      type (Domains)
      min (Optional) max (One)
    }
  }
  output (Articles)
}